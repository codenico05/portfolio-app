{
  "arrowParens": "always",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "jsxSingleQuote": false,
  "quoteProps": "as-needed",
  "singleQuote": true,
  "semi": true,
  "printWidth": 80,
  "useTabs": false,
  "tabWidth": 2,
  "trailingComma": "es5",
  "importOrder": [
    "^react$",
    "^react/(.*)$",
    "<THIRD_PARTY_MODULES>",
    "^@src/(.*)$",
    "^[./]"
  ],
  "importOrderSortSpecifiers": true
}
